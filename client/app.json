{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "94f0d011-1adf-4439-b1a3-3a6ff68d57fd"
      }
    },
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pryntik.barcodescannerclient",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/splash.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow barecode_scanner to access your camera."
        }
      ]
    ]
  }
}
